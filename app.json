      "bundleIdentifier": "com.aphrx.brdi"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ef000827-4f8c-45b7-af7e-4888fb45ba6a"
      }
    }
  }
}
